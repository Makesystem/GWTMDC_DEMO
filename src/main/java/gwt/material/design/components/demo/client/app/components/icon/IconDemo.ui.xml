<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.components.client.ui">

	<ui:with field='vMaskers'
		type='gwt.material.design.components.client.masker.Masker.Defaults' />

	<ui:with field='validations'
		type='gwt.material.design.components.client.validation.ValidationForTextField' />

	<ui:style>
	
	</ui:style>

	<m:MaterialLayoutGrid>
		<m:MaterialLayoutInner>

			<m:MaterialLayoutCell cols="12" flexDirection="COLUMN"
				marginBottom="24">
				<m:MaterialTextField ui:field="filter_text_field"
					placeholder="Find your icon" type="SHAPED" dense="true" />
				<m:MaterialContent width="100%" flexAlignItems="CENTER" flexAlignContent="CENTER">
					<m:MaterialLabel text="Icons" typography="HEADLINE_6" flex="ONE"/>
					<m:MaterialCheckbox ui:field="enabled_check" text="Enabled" selected="true" />
				</m:MaterialContent>
				<m:MaterialDivider />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">
				<m:MaterialContent ui:field="content" flexWrap="WRAP"
					flexAlignItems="START" />
			</m:MaterialLayoutCell>
		</m:MaterialLayoutInner>
	</m:MaterialLayoutGrid>

</ui:UiBinder> 