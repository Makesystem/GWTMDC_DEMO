<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.components.client.ui">

	<ui:with field='vMaskers'
		type='gwt.material.design.components.client.masker.Masker.Defaults' />

	<ui:with field='validations'
		type='gwt.material.design.components.client.validation.ValidationForTextField' />

	<ui:style>
	
	</ui:style>

	<m:MaterialLayoutGrid>
		<m:MaterialLayoutInner>

			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN" marginBottom="24">
				<m:MaterialLabel text="Chips"
					typography="HEADLINE_6" />
				<m:MaterialDivider />
			</m:MaterialLayoutCell>

			<!-- Standard -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialLabel text="Standard Chip Set"
					typography="BODY_1" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet>
					<m:MaterialChip text="Chip 1" />
					<m:MaterialChip text="Chip 2" type="OUTLINE" />
					<m:MaterialChip text="Chip 3" enabled="false" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- Input -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Input Chip Set"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet type="INPUT">
					<m:MaterialChip text="Chip 1" />
					<m:MaterialChip text="Chip 2" />
					<m:MaterialChip text="Chip 3" enabled="false" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- Choice -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Choice Chip Set"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet type="CHOICE">
					<m:MaterialChip text="Chip 1" selected="true"
						type="OUTLINE" />
					<m:MaterialChip text="Chip 2" type="OUTLINE" />
					<m:MaterialChip text="Chip 3" enabled="false"
						type="OUTLINE" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- Filter -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Filter Chip Set"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet type="FILTER">
					<m:MaterialChip text="Chip 1" selected="true"
						type="OUTLINE" />
					<m:MaterialChip text="Chip 2" type="OUTLINE" />
					<m:MaterialChip text="Chip 3" enabled="false"
						type="OUTLINE" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- Icon -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Leading icon"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet>
					<m:MaterialChip text="Chip 1" icon="FACE"
						selected="true" type="OUTLINE" />
					<m:MaterialChip text="Chip 2" icon="FACE" />
					<m:MaterialChip text="Chip 3" icon="FACE"
						enabled="false" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- Image -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Leading image"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">

				<m:MaterialChipSet>
					<m:MaterialChip text="Chip 1"
						url="https://material-components.github.io/material-components-web-catalog/static/media/photos/3x2/2.jpg"
						selected="true" type="OUTLINE" />
					<m:MaterialChip text="Chip 2"
						url="https://material-components.github.io/material-components-web-catalog/static/media/photos/3x2/2.jpg" />
					<m:MaterialChip text="Chip 3"
						url="https://material-components.github.io/material-components-web-catalog/static/media/photos/3x2/2.jpg"
						enabled="false" />
				</m:MaterialChipSet>

			</m:MaterialLayoutCell>

			<!-- To Do Tests -->
			<m:MaterialLayoutCell cols="12"
				flexDirection="COLUMN">
				<m:MaterialDivider />
				<m:MaterialLabel text="Do your tests"
					typography="BODY_1" marginTop="16" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell flexDirection="COLUMN"
				desktopCols="2" tabletCols="3" phoneCols="4">
				<m:MaterialCheckbox ui:field="enabled_check"
					text="Enabled" selected="true" />
				<m:MaterialCheckbox ui:field="checkmarker_check"
					text="Show Checkmark" selected="true" />
				<m:MaterialCheckbox ui:field="closable_check"
					text="Closable" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell flexDirection="COLUMN"
				desktopCols="2" tabletCols="3" phoneCols="4">
				<m:MaterialSelect ui:field="type_set_select"
					label="Set type" width="100%" marginBottom="16" />
				<m:MaterialSelect ui:field="type_select"
					label="Chip type" width="100%" />
			</m:MaterialLayoutCell>

			<m:MaterialLayoutCell cols="12">
				<m:MaterialChipSet ui:field="to_do_tests_element">
					<m:MaterialChip text="Chip 1" />
					<m:MaterialChip text="Chip 2" />
					<m:MaterialChip text="Chip 3" />
				</m:MaterialChipSet>
			</m:MaterialLayoutCell>

		</m:MaterialLayoutInner>
	</m:MaterialLayoutGrid>

</ui:UiBinder> 